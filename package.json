        "mobx": "^6.0.4",
        "mobx-react-lite": "^3.1.6",
        "monaco-editor-webpack-plugin": "^2.1.0",
        "monaco-themes": "^0.3.3",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-feather": "^2.0.8",
        "react-monaco-editor": "^0.41.2",
        "react-router-dom": "^5.2.0",
        "react-transition-group": "^4.4.1",
        "simple-peer": "^9.6.2",
        "socket.io": "^3.0.1",
        "socket.io-client": "^3.0.1"
    },
    "devDependencies": {
        "@babel/runtime": "^7.12.5",
