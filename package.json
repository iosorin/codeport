{
  "name": "codeport",
  "version": "1.0.0",
  "description": "Collaborative coding service",
  "author": "Osorina irina",
  "license": "ISC",
  "scripts": {
    "start": "concurrently -k -p \"[{name}]\" -n \"Client,Server\" -c \"yellow.bold,blue.bold\" \"npm run start:client\" \"npm run start:server\"",
    "start:client": "cross-env NODE_ENV=development webpack serve  --inline --hot --config config/webpack.dev.js ",
    "start:server": "nodemon --config config/nodemon.config.json",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "build:analyze": "cross-env NODE_ENV=production analyze=true webpack --progress --config config/webpack.prod.js",
    "test": "jest --config config/jest/jest.config.js",
    "test:watch": "jest --watch --config config/jest/jest.config.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx src",
    "eslint:dump": "eslint --print-config ./.eslintrc.json",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,scss,md}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run eslint",
      "git add"
    ],
    "src/**/*.{json,css,scss,md}": [
      "npm run format",
      "git add"
    ]
  },
  "dependencies": {
    "@uiw/react-codemirror": "^3.0.1",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "codemirror": "^5.58.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mobx": "^6.0.4",
    "mobx-react-lite": "^3.1.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.8",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1",
    "recharts": "^2.0.3",
    "rlc-typescript": "^1.0.2",
    "simple-peer": "^9.6.2",
    "socket.io": "^3.0.1",
    "socket.io-client": "^3.0.1"
  },
  "devDependencies": {
    "@babel/runtime": "^7.12.5",
    "@storybook/addon-actions": "^6.1.0-beta.2",
    "@storybook/addon-essentials": "^6.1.0-beta.2",
    "@storybook/addon-links": "^6.1.0-beta.2",
    "@storybook/react": "^6.1.0-beta.2",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.1",
    "@types/classnames": "^2.2.11",
    "@types/codemirror": "0.0.98",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.15",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-transition-group": "^4.4.0",
    "@types/simple-peer": "^9.6.1",
    "@types/socket.io": "^2.1.12",
    "@types/socket.io-client": "^1.4.34",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-remove-imports": "^1.4.1",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^6.2.1",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "eslint-webpack-plugin": "^2.2.1",
    "html-webpack-plugin": "^5.3.0",
    "husky": "^5.0.0-beta.0",
    "incstr": "^1.2.3",
    "jest": "^26.6.2",
    "lint-staged": "^10.5.1",
    "mini-css-extract-plugin": "^1.2.1",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.6",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^4.0.4",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
